header.location-header.clearfix
    section.location-map
        #location-map(data-href="ajax/map/data-maker.json")
        a(href="javascript:;", class="btn btn-red view_space") View space list
    // END: MAPs
    section.location-list
        .box-filter
            a(href="javascript:;", class="a_filter_close") Hide Filters
            .box-inner
                form(class="frm-adv-filer")
                    .section-filter
                        h3 Type of Space:
                        ul
                            li
                                input(type="radio", name="rd-space", id="rd-parking")
                                label(for="rd-parking") Leasing
                            li
                                input(type="radio", name="rd-space", id="rd-garages")
                                label(for="rd-garages") Garages
                            li
                                input(type="radio", name="rd-space", id="rd-all", checked="checked")
                                label(for="rd-all") All Spaces
                    // END : FILTER
                    .section-filter
                        h3 Only show spaces with:
                        ul
                            li
                                input(type="checkbox", name="chk-space", id="chk-access")
                                label(for="chk-access") 24/7 Access
                            li
                                input(type="checkbox", name="chk-space", id="chk-shelter")
                                label(for="chk-shelter") Sheltered leasing
                            li
                                input(type="checkbox", name="chk-space", id="chk-security")
                                label(for="chk-security") Security Gates
                            li
                                input(type="checkbox", name="chk-space", id="chk-airport")
                                label(for="chk-airport") Airport transfes
                            li
                                input(type="checkbox", name="chk-space", id="chk-all")
                                label(for="chk-all") Allocated space
                    // END : FILTER
        // END: FILTER ADVANCES
        .inner
            section.blk-head-locat
                section.lct-head-title
                    h2
                        i.fa.fa-map-marker
                        | Cardiff Airport Leasing
                    a(href="javascript:;", class="a_filter") Advanced Filters
                // END : TTL
                section.lct-type
                    .row
                        section.col-xs-6.type-itm
                            .box
                                input(type="checkbox", name="fil-month", id="fil-month")
                                label(for="fil-month") Monthly Leasing
                        // END : ITM
                        section.col-xs-6.type-itm
                            .box
                                input(type="checkbox", name="fil-short", id="fil-short")
                                label(for="fil-short") Short-term Leasing
                        // END : ITM
                // END : TYPE
                section.lct-ranger
                    .lct-price-ranger.clearfix
                        label Price per 100 square feet
                        #price-ranger(data-min="20", data-max="50")
                    // END : PRICE
                    .lct-square-ranger.clearfix
                        label Square feet
                        #square-ranger(data-min="20000", data-max="50000")
                    // END : PRICE
                // END : RANGER
